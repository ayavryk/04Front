{
  "name": "V4Front",
  "version": "0.0.1",
  "private": true,
  "config": {
    "platform": "pc",
    "resolution": "fhd",
    "hmr": false
  },
  "scripts": {
    "start": "better-npm-run start",
    "storybook": "start-storybook -p 1962 -c .storybook",
    "storybuild": "build-storybook -c .storybook -o tmp",
    "prebuild": "rm -rf dist/$npm_package_version/$npm_package_config_platform",
    "build": "better-npm-run build",
    "lint": "npm run tslint && npm run eslint && npm run stylelint",
    "tslint": "tslint \"app/**/*.ts\" \"app/**/*.tsx\" \"specs/**/*.ts\" \"specs/**/*.tsx\" \"stories/**/*.tsx\" --format verbose",
    "eslint": "eslint --ext .js --ext .jsx ./ .storybook/ ./jest",
    "stylelint": "stylelint \"app/**/*.css\" \".storybook/**/*.css\" \"stories/**/*.css\"",
    "test": "jest --config jest/config.json"
  },
  "lint-staged": {
    "*.@(ts|tsx)": "tslint",
    "*.@(js|jsx)": "eslint",
    "*.css": "stylelint"
  },
  "betterScripts": {
    "start": {
      "command": "node ./webpack/dev.server.js",
      "env": {
        "NODE_ENV": "development",
        "PORT": 1961,
        "PROXY": false,
        "PROXY_PORT": 5050,
        "PC_DUID": "RLCHC5ZTMIEX6"
      }
    },
    "build": {
      "command": "node --harmony $(npm bin)/webpack --colors --display-error-details --config ./webpack/prod.config.js",
      "env": {
        "NODE_ENV": "production"
      }
    },
    "proxy": {
      "command": "node --harmony ./webpack/proxy.js",
      "env": {
        "NODE_ENV": "production",
        "PROXY_PORT": 5050
      }
    }
  },
  "author": "V4Front",
